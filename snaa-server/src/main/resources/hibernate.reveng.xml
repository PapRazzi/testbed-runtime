<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<type-mapping>
		<!-- <sql-type jdbc-type="BIGINT" hibernate-type="java.lang.Integer"/> -->
		<sql-type jdbc-type="BIGINT" hibernate-type="java.lang.Long" />
		<sql-type jdbc-type="SMALLINT" hibernate-type="java.lang.Integer" />
		<sql-type jdbc-type="TINYINT" hibernate-type="java.lang.Integer" />
		<sql-type jdbc-type="FLOAT" hibernate-type="java.lang.Float" />
		<sql-type jdbc-type="TIME" hibernate-type="java.util.Date" />
	</type-mapping>
	

	<table-filter match-name=".*" package="de.uniluebeck.itm.tr.snaa.shiro.entity" />
	
	<table name="ACTIONS" catalog="trauth" class="de.uniluebeck.itm.tr.snaa.shiro.entity.Action"/>
	<table name="PERMISSIONS" catalog="trauth" class="de.uniluebeck.itm.tr.snaa.shiro.entity.Permission"/>
	<table name="RESOURCEGROUPS" catalog="trauth" class="de.uniluebeck.itm.tr.snaa.shiro.entity.ResourceGroup"/>
	<table name="ROLES" catalog="trauth" class="de.uniluebeck.itm.tr.snaa.shiro.entity.Role"/>
	<table name="URN_RESOURCEGROUPS" catalog="trauth" class="de.uniluebeck.itm.tr.snaa.shiro.entity.UrnResourceGroup"/>
	<table name="USERS" catalog="trauth" class="de.uniluebeck.itm.tr.snaa.shiro.entity.User"/>
	

</hibernate-reverse-engineering>
